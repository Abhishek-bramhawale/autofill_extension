{
    "manifest_version": 3,
    "name": "Autofill Forms Extension",
    "version": "1.0.0",
    "description": "form autofill tool that works with all types of Forms supporting all websites",
    
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    
    "action": {
      "default_popup": "mainUi/dist/index.html",
      "default_title": "Autofill Forms"
    }
  }